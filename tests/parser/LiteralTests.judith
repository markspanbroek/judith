object LiteralTests := Object |[

    object parser := ParserGenerator.literal("foo")

    method parsingSucceeds [
        object parseResult := parser.parse("foo")
        Assert.that(parseResult : List)
        Assert.that(parseResult.length = 1)
        Assert.that(parseResult.get(0) = "foo")
    ]
    
    method parsingFails [
        Assert.that(parser.parse("bar") = null)
    ]
    
]|

LiteralTests
    .parsingSucceeds
    .parsingFails
